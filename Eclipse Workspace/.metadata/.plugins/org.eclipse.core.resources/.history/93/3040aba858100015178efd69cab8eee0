
public class State {
	
	private double value;
	private double time;
	
	public State(double val, double time){
		this.value = val;
		this.time = time;
	}
	
	public State(State input){
		this.value = input.getVal();
		this.time = input.getTime();
	}
	
	public double getTime(){
		return time;
	}
	
	public double getVal(){
		return value;
	}
	
	public void set(State state){
		this.value = state.getVal();
		this.time = state.getTime();
	}

}
